{
  "vector_search": {
    "embedding_dimension": 384,
    "default_k": 100,
    "min_similarity_threshold": 0.2,
    "adaptive_similarity": true,
    "batch_size": 100,
    "index_type": "IndexFlatIP",
    "similarity_thresholds": {
      "aggregation": 0.25,
      "descriptive": 0.3,
      "analytical": 0.35,
      "default": 0.3
    }
  },
  "query_processing": {
    "max_results_for_gpt": 25,
    "max_initial_retrieval": 500,
